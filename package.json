{
  "name": "com.notepado.notepado",
  "displayName": "Notepado",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "webpack.config.js",
  "scripts": {
    "cordova-platform-add-android": "cordova platform add android",
    "build": "webpack --mode production",
    "build-cordova": "webpack --mode production && cordova build android",
    "build-all": "npm run build && npm run build-cordova",
    "build-all-prod": "webpack --mode production && cordova build android --release --buildConfig=build.json",
    "start": "webpack serve --mode development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-translations": "node updateTranslations.js",
    "run-in-smartphone": "cordova run android --device",
    "copy-and-overwrite": "node copy.js uk.json uk-en-irregular-verbs-full.json frontDesc"
  },
